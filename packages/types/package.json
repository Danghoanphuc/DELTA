{
  "name": "@printz/types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models/*": {
      "import": "./dist/models/*.js",
      "types": "./dist/models/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "tsc"
  },
  "dependencies": {
    "mongoose": "^8.0.0",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "typescript": "^5.9.3"
  }
}
