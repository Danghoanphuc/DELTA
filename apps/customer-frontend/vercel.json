{
  "buildCommand": "pnpm --filter @printz/types build && pnpm --filter customer-frontend build",
  "outputDirectory": "dist",
  "installCommand": "corepack enable pnpm && corepack prepare pnpm@10.2.0 --activate && pnpm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
