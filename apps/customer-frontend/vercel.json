{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "installCommand": "chmod +x ./prepare-vercel.sh && ./prepare-vercel.sh",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://delta-customer.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
